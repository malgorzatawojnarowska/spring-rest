<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="spring.Travel">
        <property name="transportation" ref="bus"/>
        <property name="accomodation" ref="hotel"/>
        <property name="name" value="Summer holiday 2023"/>
    </bean>

    <bean id="bus" class="spring.impl.Bus">
    </bean>

    <bean id="hotel" class="spring.impl.Hotel">
    </bean>

</beans>